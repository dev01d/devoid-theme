{
  "name": "devoid-theme",
  "displayName": "Devoid theme",
  "description": "A dark theme for VS Code with italics support",
  "version": "1.3.0",
  "publisher": "dev01d",
  "icon": ".github/assets/images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/dev01d/devoid-theme"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#515D66",
    "theme": "dark"
  },
  "keywords": [
    "theme",
    "dark",
    "italics",
    "dev01d",
    "devoid"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Devoid",
        "uiTheme": "vs-dark",
        "path": "./themes/devoid.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package --yarn",
    "release": "vsce publish --yarn",
    "release-ovsx": "ovsx publish -p $OPEN_VSX_TOKEN --yarn"
  },
  "devDependencies": {
    "vsce": "^2.15.0",
    "ovsx": "^0.9.4"
  }
}
